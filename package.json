{
  "name": "users-list",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "18.17.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint \"**/*.{ts,tsx}\"",
